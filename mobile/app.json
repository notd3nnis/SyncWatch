{
  "expo": {
    "name": "SyncWatch",
    "slug": "SyncWatch",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "syncwatch",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#0A0A0A",
    "newArchEnabled": true,
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "icon": "./src/assets/images/Icons/AppleIcon.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.notd3nnis.SyncWatch",
      "userInterfaceStyle": "dark",
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": false,
        "UIStatusBarStyle": "UIStatusBarStyleLightContent"
      }
    },
    "android": {
      "package": "com.notd3nnis.SyncWatch",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/Icons/AndroidIcon.png",
        "backgroundColor": "#0A0A0A"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "userInterfaceStyle": "dark",
      "statusBar": {
        "barStyle": "light-content",
        "backgroundColor": "#0A0A0A"
      }
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0A0A0A",
          "image": "./src/assets/images/splashScreen.png",
          "imageWidth": 55
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/dm-sans/"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "abf0380d-38ba-4832-8f4f-080d737e6c41"
      }
    }
  }
}