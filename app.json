{
  "expo": {
    "name": "SyncWatch",
    "slug": "SyncWatch",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "syncwatch",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#0A0A0A",
    "newArchEnabled": true,
    "ios": {
      "icon": "./src/assets/images/Icons/AppleIcon.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.notd3nnis.SyncWatch",
      "userInterfaceStyle": "dark"
    },
    "android": {
      "adaptiveIcon": {
    "foregroundImage": "./src/assets/images/Icons/AndroidIcon.png", 
    "backgroundColor": "#0A0A0A"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.notd3nnis.SyncWatch",
      "userInterfaceStyle": "dark",
      "statusBar": {
        "barStyle": "light-content",
        "backgroundColor": "#0A0A0A"
      }
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0A0A0A",
          "image": "./src/assets/images/splashScreen.png",
          "imageWidth": 55
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["node_modules/@expo-google-fonts/dm-sans/"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
